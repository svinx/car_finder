<div class="contact-header">
  <h2>Sign Up for New Dealers</h2>
  <br />
</div>
<div class="sign-up-form">
  <%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
    <%= devise_error_messages! %>

    <div><%= f.label :email %>
    <%= f.email_field :email %></div>

  <div><%= f.label :phone %>
  <%= f.text_field :phone %></div> 

  <div><%= f.label :"Cell phone (that you want to receive texts to)"%>
  <%= f.text_field :cell_phone %></div> 

<div><%= f.label :dealership %>
<%= f.text_field :dealership %></div> 

   <div><%= f.label :address %>
   <%= f.text_field :address %></div>

 <div><%= f.label :city %>
 <%= f.text_field :city %></div>

  <div><%= f.label :state %>
  <%= f.text_field :state %></div>

<div><%= f.label :zip %>
<%= f.text_field :zip %></div>

  <div><%= f.label :password %>
  <%= f.password_field :password %></div>

<div><%= f.label :password_confirmation %>
<%= f.password_field :password_confirmation %></div>
  <br />
  <div class="email-dealer-button">
    <%= f.submit "Sign Up", :class => "btn btn-large btn-block btn-primary" %>
  </div>
<% end %>
<%= render "devise/shared/links" %>
</div>

